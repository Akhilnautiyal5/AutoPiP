{
  "name": "Picture-in-Picture Extension (by Google)",
  "description": "Watch video using Picture-in-Picture",
  "version": "1.11",
  "icons": {
    "128": "assets/icon128.png"
  },
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "19": "assets/icon19.png",
      "38": "assets/icon38.png"
    }
  },
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "windows": "Alt+P",
        "mac": "Alt+P",
        "chromeos": "Alt+P",
        "linux": "Alt+P"
      }
    }
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "permissions": [
    "scripting",
    "storage"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "web_accessible_resources": [{
    "resources": ["ga.js"],
    "matches": ["<all_urls>"]
  }],
  "minimum_chrome_version": "69.0.3483.0",
  "manifest_version": 3
}
